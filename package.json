{
  "name": "@fuzzysaj/alma-analytics-fetch",
  "version": "1.0.0",
  "scripts": {
    "prepare": "tsc",
    "lint": "tslint \"./{src,test}/**/*.ts\"",
    "cmd-line-lookup": "DEBUG=alma-analytics-fetch ts-node src/api-key-test.ts",
    "pretest": "tsc -p .",
    "api-key-test": "DEBUG=alma-analytics-fetch ts-node -r dotenv/config src/api-key-test.ts",
    "get-table-test": "DEBUG=alma-analytics-fetch ts-node -r dotenv/config src/get-table-test.ts",
    "test": "nyc mocha",
    "posttest": "nyc report --reporter=json"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": false
  },
  "license": "GPL-3.0",
  "repository": "fuzzysaj/alma-analytics-fetch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "fuzzysaj",
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.5",
    "@types/chai-as-promised": "^7.1.2",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.6.1",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
