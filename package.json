{
  "name": "@fuzzysaj/alma-analytics-fetch",
  "version": "1.0.3",
  "scripts": {
    "prepare": "tsc",
    "lint": "tslint \"./{src,test}/**/*.ts\"",
    "cmd-line-lookup": "DEBUG=alma-analytics-fetch ts-node src/api-key-test.ts",
    "pretest": "tsc -p .",
    "api-key-test": "DEBUG=alma-analytics-fetch ts-node -r dotenv/config src/api-key-test.ts",
    "get-table-test": "DEBUG=alma-analytics-fetch ts-node -r dotenv/config src/get-table-test.ts",
    "test": "nyc mocha",
    "posttest": "nyc report --reporter=json"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": false
  },
  "license": "GPL-3.0",
  "repository": "fuzzysaj/alma-analytics-fetch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "fuzzysaj",
  "dependencies": {
    "axios": "^0.25.0",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.14",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.2",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.0",
    "typescript": "^4.5.5"
  }
}
